alias so='source ~/.bashrc'

# See /usr/share/doc/bash-doc/examples in the bash-doc package.
alias ..='cd ../'
alias ...='cd ../../'
alias .3='cd ../../../'
alias .4='cd ../../../../'
alias .5='cd ../../../../../'
alias .6='cd ../../../../../../'
alias DT='tee ~/Desktop/terminalOut.txt'
alias apacheEdit='sudo edit /etc/httpd/httpd.conf'
alias apacheLogs='less +F /var/log/apache2/error_log'
alias apacheRestart='sudo apachectl graceful'
alias c='clear'
alias cat='/usr/local/bin/ccat'
alias cd..='cd ../'
alias cic='set completion-ignore-case On'
alias cleanupDS='find . -type f -name '\''*.DS_Store'\'' -ls -delete'
alias cleanupLS='/System/Library/Frameworks/CoreServices.framework/Frameworks/LaunchServices.framework/Support/lsregister -kill -r -domain local -domain system -domain user && killall Finder'
alias cp='cp -iv'
alias cpu_hogs='ps wwaxr -o pid,stat,%cpu,time,command | head -10'
alias edit='subl'
alias editHosts='sudo vi /etc/hosts'
alias f='open -a Finder ./'
alias finderHideHidden='defaults write com.apple.finder ShowAllFiles FALSE'
alias finderShowHidden='defaults write com.apple.finder ShowAllFiles TRUE'
alias fix_stty='stty sane'
alias flushDNS='dscacheutil -flushcache'
alias herr='tail /var/log/httpd/error_log'
alias ipInfo0='ipconfig getpacket en0'
alias ipInfo1='ipconfig getpacket en1'
alias less='less -FSRXc'
alias ll='ls -FGlAhp'
alias llr='ls -FGlAhpirt'
alias lr='ls -R | grep ":$" | sed -e '\''s/:$//'\'' -e '\''s/[^-][^\/]*\//--/g'\'' -e '\''s/^/   /'\'' -e '\''s/-/|/'\'' | less'
alias lsock='sudo /usr/sbin/lsof -i -P'
alias lsockT='sudo /usr/sbin/lsof -nP | grep TCP'
alias lsockU='sudo /usr/sbin/lsof -nP | grep UDP'
alias make10mb='mkfile 10m ./10MB.dat'
alias make1mb='mkfile 1m ./1MB.dat'
alias make5mb='mkfile 5m ./5MB.dat'
alias memHogsPs='ps wwaxm -o pid,stat,vsize,rss,time,command | head -10'
alias memHogsTop='top -l 1 -o rsize | head -20'
alias mkdir='mkdir -pv'
alias mountReadWrite='/sbin/mount -uw /'
alias mv='mv -iv'
alias myip='curl ip.appspot.com'
alias netCons='lsof -i'
alias numFiles='echo $(ls -1 | wc -l)'
alias openPorts='sudo lsof -i | grep LISTEN'
alias path='echo -e ${PATH//:/\\n}'
alias python='/usr/bin/python'
alias qfind='find . -name '
alias screensaverDesktop='/System/Library/Frameworks/ScreenSaver.framework/Resources/ScreenSaverEngine.app/Contents/MacOS/ScreenSaverEngine -background'
alias showBlocked='sudo ipfw list'
alias show_options='shopt'
alias topForever='top -l 9999999 -s 10 -o cpu'
alias ttop='top -R -F -s 10 -o rsize'
alias which='type -all'
alias ~='cd ~'
alias asm='~/AMIGA-Code/vasmm68k/vasmm68k_mot'
